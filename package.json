    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "postbuild": "next-sitemap",
        "start": "next start",
        "lint": "next lint",
        "db:generate": "dotenvx run -f .env.local -- run-s auth:generate",
        "db:migrate": "dotenvx run -f .env.local -- run-s drizzle:generate drizzle:migrate",
        "db:push": "dotenvx run -f .env.local -- run-s drizzle:push",
        "drizzle:push": "drizzle-kit push",
        "drizzle:generate": "drizzle-kit generate",
        "drizzle:migrate": "drizzle-kit migrate",
        "auth:generate": "cli generate --config=./better-auth.config.ts --output=./src/lib/server/db/schema/auth.ts --yes",
        "auth:migrate": "cli migrate --config=./better-auth.config.ts --yes"
    },
